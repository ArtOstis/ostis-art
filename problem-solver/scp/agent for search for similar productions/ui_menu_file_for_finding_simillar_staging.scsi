ui_menu_file_for_finding_simillar_staging
	<- 	ui_user_command_class_atom; 
		ui_user_command_class_view_kb;
	=> nrel_main_idtf: 
		[Какие существуют похожие постановки?](* <- lang_ru;; *);
		[What simillar staging are exist?] (* <- lang_en;; *);
	=> ui_nrel_command_template:
		[*
		question_finding_simillar_staging _-> ._question_finding_simillar_staging_instance
			(*
			_-> ui_arg_1;;
			*);;
		._question_finding_simillar_staging_instance _<- question;;
		*];
	=> ui_nrel_command_lang_template: 
		[Запрос поиска похожих постановок для: $ui_arg_1] 
		(* <- lang_ru;; *);
		[Request of finding simillar staging for: $ui_arg_1] 
		(* <- lang_en;; *);;

